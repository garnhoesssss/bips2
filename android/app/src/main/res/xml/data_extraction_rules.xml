<?xml version="1.0" encoding="utf-8"?>
<!--
  Data Extraction Rules - Android 12+ (API 31+)
  Required for android:dataExtractionRules attribute
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all data from cloud backup for security -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude all data from device-to-device transfer -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>
